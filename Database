create databse CarteX;

use CarteX; 

CREATE TABLE carte (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nom VARCHAR(255) NOT NULL,
    type VARCHAR(50),
    image_url VARCHAR(255),
    niveau INT,
    description TEXT,
    prix DECIMAL(10, 2),
    rarete VARCHAR(20)
);

CREATE TABLE user (
    id INT AUTO_INCREMENT PRIMARY KEY,
    identifiant VARCHAR(255) NOT NULL,
    pwd VARCHAR(255) NOT NULL
    );
