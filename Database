create database CarteX;

use CarteX; 

CREATE TABLE carte (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nom VARCHAR(255) NOT NULL,
    type VARCHAR(50),
    image_carte VARCHAR(255),
    niveau INT,
    description TEXT,
    Prix DECIMAL(10, 2),
    rarete VARCHAR(255)
);

CREATE TABLE user (
    id INT AUTO_INCREMENT PRIMARY KEY,
    identifiant VARCHAR(255) NOT NULL,
    pwd VARCHAR(255) NOT NULL,
    rang Varchar(225)
    );

create table deck(
    id INT AUTO_INCREMENT PRIMARY KEY,
    id_carte INT,
    id_user INT);


